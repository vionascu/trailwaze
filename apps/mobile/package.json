{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "e2e:build:ios": "detox build -c ios.sim.debug",
    "e2e:test:ios": "detox test -c ios.sim.debug",
    "e2e:build:android": "detox build -c android.emu.debug",
    "e2e:test:android": "detox test -c android.emu.debug"
  },
  "dependencies": {
    "@maplibre/maplibre-react-native": "^10.4.2",
    "expo": "~54.0.31",
    "expo-asset": "~12.0.12",
    "expo-file-system": "~19.0.21",
    "expo-location": "~19.0.8",
    "expo-status-bar": "~3.0.9",
    "maplibre-gl": "^5.7.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "detox": "^20.18.1",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "jest-expo": "^54.0.9",
    "jest-jasmine2": "^29.7.0",
    "react-test-renderer": "19.1.0",
    "source-map": "^0.6.1"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": ["**/__tests__/**/*.test.js"],
    "moduleFileExtensions": ["js", "json"],
    "transform": {},
    "testRunner": "jest-circus/runner",
    "coverageProvider": "v8",
    "collectCoverageFrom": ["**/*.js", "!**/node_modules/**", "!**/coverage/**"]
  },
  "detox": {
    "testRunner": "jest",
    "runnerConfig": "e2e/jest.config.js",
    "configurations": {
      "ios.sim.debug": {
        "type": "ios.simulator",
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/mobile.app",
        "build": "expo prebuild --clean --platform ios && xcodebuild -workspace ios/mobile.xcworkspace -scheme mobile -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "device": {
          "type": "iPhone 15"
        }
      },
      "android.emu.debug": {
        "type": "android.emulator",
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "expo prebuild --clean --platform android && cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug",
        "device": {
          "avdName": "Pixel_6_API_34"
        }
      }
    }
  },
  "private": true
}
